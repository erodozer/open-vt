; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="open-vt"
run/main_scene="res://studio/studio.tscn"
config/features=PackedStringArray("4.5", "Forward Plus")
run/max_fps=60
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/fullsize=false
boot_splash/image="uid://bcqaflbakxadd"
config/icon="uid://do3gvkipu7gp4"
boot_splash/minimum_display_time=2000
boot_splash/image.editor_hint=""

[audio]

driver/enable_input=true

[autoload]

GlobalInput="*res://addons/keylogger/keylogger.tscn"
Screenshot="*res://lib/screenshot.gd"
Registry="*res://lib/tracking/parameter_registry.gd"
Preferences="*res://lib/preferences.gd"
ModelManager="*res://lib/model_manager.gd"
ItemManager="*res://lib/item_manager.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/transparent=true
window/subwindows/embed_subwindows=false
window/stretch/aspect="expand"
window/per_pixel_transparency/allowed=true

[editor_plugins]

enabled=PackedStringArray("res://addons/keylogger/plugin.cfg")

[global_group]

system:tracking=""
system:model=""
persist="Accesses and Stores state within application config"
system:hotkey=""
vtmodel=""
system:items=""
system:stage=""
system:settings=""
hide_screenshot="Removes element from viewport when taking a screenshot"
system:alert=""
persist:model="Write values into openvt.json with model files"

[gui]

theme/custom="res://ui/ui_theme.tres"

[importer_defaults]

gd_cubism.model={
"Load Expressions": false,
"Load Motion": false
}
texture={
"compress/channel_pack": 0,
"compress/hdr_compression": 1,
"compress/high_quality": false,
"compress/lossy_quality": 0.7,
"compress/mode": 4,
"compress/normal_map": 0,
"detect_3d/compress_to": 1,
"editor/convert_colors_with_editor_theme": false,
"editor/scale_with_editor_scale": false,
"mipmaps/generate": true,
"mipmaps/limit": -1,
"process/fix_alpha_border": true,
"process/hdr_as_srgb": false,
"process/hdr_clamp_exposure": false,
"process/normal_map_invert_y": false,
"process/premult_alpha": false,
"process/size_limit": 0,
"roughness/mode": 0,
"roughness/src_normal": "",
"svg/scale": 1.0
}

[input]

ui_screenshot={
"deadzone": 0.2,
"events": []
}

[rendering]

textures/canvas_textures/default_texture_filter=2
environment/defaults/default_clear_color=Color(0.301961, 0.301961, 0.301961, 0)
viewport/transparent_background=true
