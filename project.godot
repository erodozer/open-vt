; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="open-vt"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
run/low_processor_mode=true
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/fullsize=false
boot_splash/image="uid://cmlu8yo3haxyo"
config/icon="uid://do3gvkipu7gp4"
boot_splash/minimum_display_time=2000
boot_splash/image.editor_hint=""

[audio]

driver/enable_input=true

[autoload]

GlobalInput="*res://addons/keylogger/keylogger.tscn"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/transparent=true
window/subwindows/embed_subwindows=false
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/per_pixel_transparency/allowed=true

[editor_plugins]

enabled=PackedStringArray("res://addons/keylogger/plugin.cfg")

[global_group]

system:tracking=""
system:model=""
persist="Accesses and Stores state within application config"
system:hotkey=""
vtmodel=""
system:items=""
system:stage=""
system:settings=""

[gui]

theme/custom="res://studio/ui/ui_theme.tres"

[importer_defaults]

gd_cubism.model={
"Load Expressions": false,
"Load Motion": false
}

[rendering]

environment/defaults/default_clear_color=Color(0.301961, 0.301961, 0.301961, 0)
